<% identifiers = lookup_displayable_identifiers(result.json['agent_record_identifiers']) %>
<% if identifiers.any? %>
  <h3><%= I18n.t('agent_record_identifier._plural') %></h3>
  <ul>
    <% identifiers.each do |id| %>
      <li>
        <a href="<%= id['record_identifier'] %>"><%= id['record_identifier'] %></a>
      </li>
    <% end %>
  </ul>
<% end %>
