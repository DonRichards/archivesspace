<% if result.json[subject_type].any? %>
<h3><%= subject_label %></h3>
<ul>
<% result.json[subject_type].each do |n| %>
  <% subjects = n['subjects'] %>

  <% subjects.each do |s| %>
    <% if s['_resolved']['publish'] %>
      <li><a href="<%= s['_resolved']['uri']%>"><%= s['_resolved']['title'] %></a></li>
    <% else %>
      <li><%= s['_resolved']['title'] %></li>
    <% end %>
  <% end %>
<% end %>
</ul>
<% end %>
